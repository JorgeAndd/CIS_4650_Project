void radixsort(int vetor[2], int tamanho) {
    int i;
    int b[5];
    int maior;
    int exp;

    for (i = 0; i < tamanho; i++) {
        if (vetor[i] > maior)
    	    maior = vetor[i];
    }

    while (maior/exp > 0) {
    	for (i = 0; i < tamanho; i++)
    	    bucket[(vetor[i] / exp) % 10]++;
    	for (i = 1; i < 10; i++)
    	    bucket[i] = bucket[i - 1];
    	for (i = tamanho - 1; i >= 0; i--)
    	    b[--bucket[(vetor[i] / exp) % 10]] = vetor[i];
    	for (i = 0; i < tamanho; i++)
    	    vetor[i] = b[i];
    	exp = exp * 10;
    }
}

int func1(int a){
    int i;
    for(i = 0; i < 10; i++){
        a = a * a;
    }

    return a;
}

